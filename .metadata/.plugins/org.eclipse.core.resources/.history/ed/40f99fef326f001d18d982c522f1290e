#ifndef _UART_H
#define _UART_H

/**
 * Read blocking a single byte
 */
int UART_Read();

/**
 * Write blocking a single byte
 */
int UART_Write(int val);

/**
 * Initialize osci
 */
int UART_Init();

#endif // _UART_H
